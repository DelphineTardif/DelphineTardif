@article{metais_new_2017,
	title = {A new assemblage of stem pecoran ruminants from the {Oligocene} {Chitarwata} {Formation}, {Bugti} {Hills}, {Baluchistan}, {Pakistan}: {Paleoenvironmental} and paleobiogeographic implications},
