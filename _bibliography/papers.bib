	pages = {1829--1844},
	file = {Castruccio et al. - 2014 - Statistical Emulation of Climate Model Projections.pdf:/Users/delphinetardif/Zotero/storage/3WBK3MV3/Castruccio et al. - 2014 - Statistical Emulation of Climate Model Projections.pdf:application/pdf}
