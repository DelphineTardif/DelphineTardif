	journal = {Global and Planetary Change},
	author = {Deng, Weiyudong and Su, Tao and Wappler, Torsten and Liu, Jia and Li, Shufeng and Huang, Jian and Tang, He and Low, Shook Ling and Wang, Tengxiang and Xu, He and Xu, Xiaoting and Liu, Ping and Zhou, Zhekun},
