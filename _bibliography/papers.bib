	note = {Publisher: Copernicus GmbH},
	pages = {703--719},
	file = {Full Text PDF:/Users/delphinetardif/Zotero/storage/IPDK85NZ/Raitzsch et al. - 2021 - Atmospheric carbon dioxide variations across the m.pdf:application/pdf;Snapshot:/Users/delphinetardif/Zotero/storage/UTZSHVIP/2021.html:text/html}
