	author = {Peppe, Daniel J. and Royer, Dana L. and Cariglino, Bárbara and Oliver, Sofia Y. and Newman, Sharon and Leight, Elias and Enikolopov, Grisha and Fernandez‐Burgos, Margo and Herrera, Fabiany and Adams, Jonathan M. and Correa, Edwin and Currano, Ellen D. and Erickson, J. Mark and Hinojosa, Luis Felipe and Hoganson, John W. and Iglesias, Ari and Jaramillo, Carlos A. and Johnson, Kirk R. and Jordan, Gregory J. and Kraft, Nathan J. B. and Lovelock, Elizabeth C. and Lusk, Christopher H. and Niinemets, Ülo and Peñuelas, Josep and Rapson, Gillian and Wing, Scott L. and Wright, Ian J.},
	year = {2011},
	keywords = {climate proxies, leaf lifespan, leaf physiognomy, paleobotany, paleoclimate, phylogeny, precipitation, temperature},
