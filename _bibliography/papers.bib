	file = {Craggs et al. - 2012 - Climate model predictions for the latest Cretaceou.pdf:/Users/delphinetardif/Zotero/storage/HD4LM7FP/Craggs et al. - 2012 - Climate model predictions for the latest Cretaceou.pdf:application/pdf}
