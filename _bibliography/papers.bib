	pages = {40--49},
	file = {Métais et al. - 2017 - A new assemblage of stem pecoran ruminants from th.pdf:/Users/delphinetardif/Zotero/storage/TNP9E5QH/Métais et al. - 2017 - A new assemblage of stem pecoran ruminants from th.pdf:application/pdf}
}

@article{jia_cenozoic_2020,
	title = {Cenozoic aridification in {Northwest} {China} evidenced by paleovegetation evolution},
	volume = {557},
	issn = {00310182},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0031018220303527},
	doi = {10.1016/j.palaeo.2020.109907},
	abstract = {Although the Cenozoic aridiﬁcation of the Asian interior was of global signiﬁcance, the timing of its evolution and the drivers remain controversial. However, knowledge of the spatiotemporal pattern of vegetation evolution can inform our understanding of the aridiﬁcation process. Here, we integrate 58 pollen records with wellconstrained chronologies from across Asia, and use the biomization method to quantitatively reconstruct the Cenozoic evolution of the vegetation. The results reveal the occurrence of desert in Northwest China during {\textasciitilde}43–35 Ma, indicating a dry climate. After {\textasciitilde}22 Ma there was a further expansion of desert and steppe in Northwest China, indicating the existence of a permanent arid environment. The aridity was intensiﬁed after {\textasciitilde}8–7 Ma, evidenced by the replacement of forest by steppe and desert. We suggest that the permanent aridiﬁcation since 22 Ma was related to the uplift of the Tibetan Plateau, and that the enhanced aridiﬁcation since {\textasciitilde}8–7 Ma was caused by ongoing global cooling.},
	language = {en},
	urldate = {2020-08-05},
	journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
	author = {Jia, Yunxia and Wu, Haibin and Zhu, Shuya and Li, Qin and Zhang, Chunxia and Yu, Yanyan and Sun, Aizhi},
	month = nov,
	year = {2020},
	pages = {109907},
	file = {Jia et al. - 2020 - Cenozoic aridification in Northwest China evidence.pdf:/Users/delphinetardif/Zotero/storage/BCPF5W8E/Jia et al. - 2020 - Cenozoic aridification in Northwest China evidence.pdf:application/pdf}
}

@article{beard_new_2016,
