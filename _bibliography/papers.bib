	file = {Costa et al. - 2009 - Closing and continentalization of the South Pyrene.pdf:/Users/delphinetardif/Zotero/storage/U6TDJCTP/Costa et al. - 2009 - Closing and continentalization of the South Pyrene.pdf:application/pdf}
