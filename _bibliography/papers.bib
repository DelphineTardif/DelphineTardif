	pages = {925--943},
	file = {Sokoloff et al. - 2018 - Staminate flower of Prunus s. l. (Rosaceae) from E.pdf:/Users/delphinetardif/Zotero/storage/GIZRLQ8V/Sokoloff et al. - 2018 - Staminate flower of Prunus s. l. (Rosaceae) from E.pdf:application/pdf}
}

