	pages = {1--17},
	file = {Shellito et Sloan - 2006 - Reconstructing a lost Eocene paradise Part I. Sim.pdf:/Users/delphinetardif/Zotero/storage/3H8IHUZK/Shellito et Sloan - 2006 - Reconstructing a lost Eocene paradise Part I. Sim.pdf:application/pdf}
}

