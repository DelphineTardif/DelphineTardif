	year = {2019},
	keywords = {Arctic gateways, Arctic Ocean, CO level, Earth system model, Late Cretaceous, Sea-ice},
	pages = {201--212},
	file = {Niezgodzki et al. - 2019 - Was the Arctic Ocean ice free during the latest Cr.pdf:/Users/delphinetardif/Zotero/storage/J76AEJBT/Niezgodzki et al. - 2019 - Was the Arctic Ocean ice free during the latest Cr.pdf:application/pdf}
}

