	pages = {131--138},
	file = {Valero et al. - 2016 - Long-period astronomically-forced terrestrial carb.pdf:/Users/delphinetardif/Zotero/storage/AXMUICY5/Valero et al. - 2016 - Long-period astronomically-forced terrestrial carb.pdf:application/pdf}
}

