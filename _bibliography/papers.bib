@article{bosmans_obliquity_2015,
	title = {Obliquity forcing of low-latitude climate},
	volume = {11},
	issn = {1814-9332},
	url = {http://www.clim-past.net/11/1335/2015/},
	doi = {10.5194/cp-11-1335-2015},
	abstract = {The inﬂuence of obliquity, the tilt of the Earth’s rotational axis, on incoming solar radiation at low latitudes is small, yet many tropical and subtropical palaeoclimate records reveal a clear obliquity signal. Several mechanisms have been proposed to explain this signal, such as the remote inﬂuence of high-latitude glacials, the remote effect of insolation changes at mid- to high latitudes independent of glacial cyclicity, shifts in the latitudinal extent of the tropics, and changes in latitudinal insolation gradients. Using a sophisticated coupled ocean–atmosphere global climate model, ECEarth, without dynamical ice sheets, we performed two idealized experiments of obliquity extremes. Our results show that obliquity-induced changes in tropical climate can occur without high-latitude ice sheet ﬂuctuations. Furthermore, the tropical circulation changes are consistent with obliquityinduced changes in the cross-equatorial insolation gradient, suggesting that this gradient may be used to explain obliquity signals in low-latitude palaeoclimate records instead of the classical 65◦ N summer insolation curve.},
	language = {en},
	number = {10},
	urldate = {2018-06-20},
	journal = {Climate of the Past},
	author = {Bosmans, J. H. C. and Hilgen, F. J. and Tuenter, E. and Lourens, L. J.},
	month = oct,
	year = {2015},
	pages = {1335--1346},
	annote = {Many changes at low lat show and obliquity signal that is not coerent with insolation changes at these lat (very week), but correponds better to 65N insolation records.
Influence from high lats are often used to explain these mechanisms (presence of ice at the poles amplifying the signal, for example).
 
Here, they show that the ISMI : the Indian summer monsoon index (I30 N–I30 S at 1 August) AND the summer inter-tropical insolation gradient (SITIG), I23 N– I23 S on 21 June correlates perfectly with the low lat  changes observe.
 
Theories :



Leuschner and Sirocko (2003) and Reichart (1996) sug- gested that through monsoon-induced changes in atmo- spheric moisture content, a strong greenhouse gas, the sum- mer inter-tropical insolation gradient (SITIG) may drive glacial–interglacial variability 
 



Raymo and Nisan- cioglu (2003), Vettoretti and Peltier (2004), Antico et al. (2010) and Mantsis (2011). These studies suggest that the poleward transport of heat, moisture and latent energy is increased during minimum obliquity due to the intensified intra-hemispheric (Equator-to-pole) insolation and tempera- ture gradient. The increased moisture transport towards the poles combined with low polar temperatures during low obliquity is favourable for ice growth. 





},
	file = {Bosmans et al. - 2015 - Obliquity forcing of low-latitude climate.pdf:/Users/delphinetardif/Zotero/storage/9YDDWHHY/Bosmans et al. - 2015 - Obliquity forcing of low-latitude climate.pdf:application/pdf}
}

