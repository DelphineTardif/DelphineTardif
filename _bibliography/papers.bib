	abstract = {Aim The geological evolution of the Mediterranean region is largely the result of the Tertiary collision of the African and Eurasian Plates, but also a mosaic of migrating island arcs, fragmenting tectonic belts, and extending back-arc basins. Such complex paleogeography has resulted in a ÔreticulateÕ biogeographical history, in which Mediterranean biotas repeatedly fragmented and merged as dispersal barriers appeared and disappeared through time. In this study, dispersal-vicariance analysis (DIVA) is used to assess the relative role played by dispersal and vicariance in shaping distribution patterns in the beetle subfamily Pachydeminae Reitter, 1902 (Scarabaeoidea), an example of east–west Mediterranean disjunction.},
	language = {en},
	number = {12},
	urldate = {2020-07-09},
	journal = {Journal of Biogeography},
	author = {Sanmartín, Isabel},
	month = dec,
	year = {2003},
	pages = {1883--1897},
	file = {Sanmartín - 2003 - Dispersal vs. vicariance in the Mediterranean his.pdf:/Users/delphinetardif/Zotero/storage/2FAYSQ4I/Sanmartín - 2003 - Dispersal vs. vicariance in the Mediterranean his.pdf:application/pdf}
}

