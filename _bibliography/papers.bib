	journal = {Journal of Plant Research},
	author = {Sokoloff, Dmitry D. and Ignatov, Michael S. and Remizowa, Margarita V. and Nuraliev, Maxim S. and Blagoderov, Vladimir and Garbout, Amin and Perkovsky, Evgeny E.},
	month = nov,
