	pages = {15--18},
	file = {Clementz et al. - 2011 - Early Eocene warming events and the timing of terr.pdf:/Users/delphinetardif/Zotero/storage/S98PN7KW/Clementz et al. - 2011 - Early Eocene warming events and the timing of terr.pdf:application/pdf}
}

