	pages = {261--277},
	file = {Braconnot et al. - 2007 - Results of PMIP2 coupled simulations of the Mid-Ho.pdf:/Users/delphinetardif/Zotero/storage/2APSAFVV/Braconnot et al. - 2007 - Results of PMIP2 coupled simulations of the Mid-Ho.pdf:application/pdf}
