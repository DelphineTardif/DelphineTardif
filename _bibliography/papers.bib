@article{clift_late_2020,
	title = {A {Late} {Eocene}-{Oligocene} {Through}-{Flowing} {River} {Between} the {Upper} {Yangtze} and {South} {China} {Sea}},
	volume = {21},
	issn = {1525-2027},
	url = {http://agupubs.onlinelibrary.wiley.com/doi/full/10.1029/2020GC009046},
	doi = {10.1029/2020GC009046},
	abstract = {Abstract We test the hypothesis of a major Paleogene river draining the SE Tibetan Plateau and the central modern Yangtze Basin that then flowed south to the South China Sea. We test this model using U-Pb dated detrital zircon grains preserved in Paleogene sedimentary rocks in northern Vietnam and SW China. We applied a series of statistical tests to compare the U-Pb age spectra of the rocks in order to highlight differences and similarities between them and with potential source bedrocks. Monte Carlo mixing models imply that erosion was dominantly derived from the Indochina and Songpan-Garzê Blocks and to a lesser extent the Yangtze Craton. Some of the zircon populations indicate local erosion and sedimentation, but others show close similarity both within northern Vietnam, as well as more widely in the Eocene Jianchuan, Paleocene-Oligocene Simao, and Oligocene-Miocene Yuanjiang basins of China. The presence of younger ({\textless}200 Ma) zircons from the Qamdo Block of Tibet is less easily explicable in terms of recycling by erosion of older sedimentary rocks and implies a regional drainage linking SE Tibet and the South China Sea in the Late Eocene-Oligocene. Detrital zircons from offshore in the South China Sea showed initial local erosion, but with a connection to a river stretching to SE Tibet in the Late Oligocene. A change from regional to local sources in the Early Miocene in the Yuanjiang Basin indicates the timing of disruption of the old drainage driven by regional plateau uplift.},
	number = {7},
	urldate = {2021-03-17},
	journal = {Geochemistry, Geophysics, Geosystems},
	author = {Clift, Peter D. and Carter, Andrew and Wysocka, Anna and Van Hoang, Long and Zheng, Hongbo and Neubeck, Nikki},
	month = jul,
	year = {2020},
	note = {Publisher: John Wiley \& Sons, Ltd},
	keywords = {Tibet, erosion, Indochina, provenance, rivers, zircon},
	pages = {e2020GC009046},
	file = {Snapshot:/Users/delphinetardif/Zotero/storage/MKSU6KPC/2020GC009046.html:text/html}
}

@article{clift_asian_2020,
	title = {Asian monsoon dynamics and sediment transport in {SE} {Asia}},
	volume = {195},
	issn = {1367-9120},
	url = {https://www.sciencedirect.com/science/article/pii/S1367912020301334},
	doi = {10.1016/j.jseaes.2020.104352},
	abstract = {The Asian monsoon is the dominant climatic phenomenon in Southeast Asia, responsible for most of the regional precipitation. As well as tectonics, climate is well recognized as a primary control on erosion and the transport of sediment to the ocean. Sediment records from the South China Sea show that strong monsoons are associated with intensified reworking of pre-existing floodplain sediments over millennial timescales. Strong monsoons result in deposition of more altered material that is also delivered at higher rates than during more arid times. Millennial-scale changes in monsoon strength result in changes in the weathering regime but not fast enough to account for the changes seen in the sediments preserved in Asian deltas. Instead monsoon modulated recycling dominates. Over longer time periods ({\textgreater}106 y) strengthening of the monsoon is linked to faster bedrock erosion and increased sediment flux of the ocean. Although this also has an effect on chemical weathering, on long time periods the monsoon is seen to be subsidiary to the effects of global cooling at least since the middle Miocene.},
	language = {en},
	urldate = {2021-03-17},
	journal = {Journal of Asian Earth Sciences},
	author = {Clift, Peter D.},
	month = jun,
	year = {2020},
	keywords = {Clay, Erosion, Monsoon, Recycling, Sediment transport, Weathering},
	pages = {104352},
	file = {ScienceDirect Full Text PDF:/Users/delphinetardif/Zotero/storage/R7ZUQ52K/Clift - 2020 - Asian monsoon dynamics and sediment transport in S.pdf:application/pdf;ScienceDirect Snapshot:/Users/delphinetardif/Zotero/storage/BUASDG4R/S1367912020301334.html:text/html}
}

