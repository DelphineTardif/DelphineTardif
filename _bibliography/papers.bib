	pages = {11--14},
	file = {Armstrong et Allen - 2011 - Shifts in the Intertropical Convergence Zone, Hima.pdf:/Users/delphinetardif/Zotero/storage/9KAHQ6EB/Armstrong et Allen - 2011 - Shifts in the Intertropical Convergence Zone, Hima.pdf:application/pdf}
}

