	journal = {Global and Planetary Change},
	author = {Taylor, Kyle W.R. and Huber, Matthew and Hollis, Christopher J. and Hernandez-Sanchez, Maria T. and Pancost, Richard D.},
	month = sep,
	year = {2013},
	pages = {158--174},
