@article{hutchinson_eoceneoligocene_2021,
	title = {The {Eocene}–{Oligocene} transition: a review of marine and terrestrial proxy data, models and model–data comparisons},
	volume = {17},
	issn = {1814-9324},
	shorttitle = {The {Eocene}–{Oligocene} transition},
	url = {https://cp.copernicus.org/articles/17/269/2021/},
	doi = {https://doi.org/10.5194/cp-17-269-2021},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} The Eocene–Oligocene transition (EOT) was a climate shift from a largely ice-free greenhouse world to an icehouse climate, involving the first major glaciation of Antarctica and global cooling occurring {\textless}span class="inline-formula"{\textgreater}∼34{\textless}/span{\textgreater} million years ago (Ma) and lasting {\textless}span class="inline-formula"{\textgreater}∼790{\textless}/span{\textgreater} {\textless}span class="inline-formula"{\textgreater}kyr{\textless}/span{\textgreater}. The change is marked by a global shift in deep-sea {\textless}span class="inline-formula"{\textgreater}\textit{δ}$^{\textrm{18}}$O{\textless}/span{\textgreater} representing a combination of deep-ocean cooling and growth in land ice volume. At the same time, multiple independent proxies for ocean temperature indicate sea surface cooling, and major changes in global fauna and flora record a shift toward more cold-climate-adapted species. The two principal suggested explanations of this transition are a decline in atmospheric {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} and changes to ocean gateways, while orbital forcing likely influenced the precise timing of the glaciation. Here we review and synthesise proxy evidence of palaeogeography, temperature, ice sheets, ocean circulation and {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} change from the marine and terrestrial realms. Furthermore, we quantitatively compare proxy records of change to an ensemble of climate model simulations of temperature change across the EOT. The simulations compare three forcing mechanisms across the EOT: {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} decrease, palaeogeographic changes and ice sheet growth. Our model ensemble results demonstrate the need for a global cooling mechanism beyond the imposition of an ice sheet or palaeogeographic changes. We find that {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} forcing involving a large decrease in {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} of ca. 40 \% ({\textless}span class="inline-formula"{\textgreater}∼325{\textless}/span{\textgreater} {\textless}span class="inline-formula"{\textgreater}ppm{\textless}/span{\textgreater} drop) provides the best fit to the available proxy evidence, with ice sheet and palaeogeographic changes playing a secondary role. While this large decrease is consistent with some {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} proxy records (the extreme endmember of{\textless}span id="page270"/{\textgreater} decrease), the positive feedback mechanisms on ice growth are so strong that a modest {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} decrease beyond a critical threshold for ice sheet initiation is well capable of triggering rapid ice sheet growth. Thus, the amplitude of {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} decrease signalled by our data–model comparison should be considered an upper estimate and perhaps artificially large, not least because the current generation of climate models do not include dynamic ice sheets and in some cases may be under-sensitive to {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} forcing. The model ensemble also cannot exclude the possibility that palaeogeographic changes could have triggered a reduction in {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater}.{\textless}/p{\textgreater}},
	language = {English},
	number = {1},
	urldate = {2021-02-24},
	journal = {Climate of the Past},
	author = {Hutchinson, David K. and Coxall, Helen K. and Lunt, Daniel J. and Steinthorsdottir, Margret and de Boer, Agatha M. and Baatsen, Michiel and von der Heydt, Anna and Huber, Matthew and Kennedy-Asser, Alan T. and Kunzmann, Lutz and Ladant, Jean-Baptiste and Lear, Caroline H. and Moraweck, Karolin and Pearson, Paul N. and Piga, Emanuela and Pound, Matthew J. and Salzmann, Ulrich and Scher, Howie D. and Sijp, Willem P. and Śliwińska, Kasia K. and Wilson, Paul A. and Zhang, Zhongshi},
	month = jan,
	year = {2021},
	note = {Publisher: Copernicus GmbH},
	pages = {269--315},
	file = {Full Text PDF:/Users/delphinetardif/Zotero/storage/S48FRXTP/Hutchinson et al. - 2021 - The Eocene–Oligocene transition a review of marin.pdf:application/pdf;Snapshot:/Users/delphinetardif/Zotero/storage/V97D6V7J/cp-17-269-2021.html:text/html}
